<div class="container">

<div class="row">
  <div class="col-lg-10">

<table class="table table-striped table-hover table-bordered">
     <thead>
       <tr>
         <th>Subject</th>
         <th>Message</th>
         <th>Creator</th>
         <th>View</th>
       </tr>
     </thead>
     <tbody>
       <% @tickets.each do |ticket| %>
       <tr>
         <td><%= ticket.subject %></td>
         <td><%= ticket.body %></td>
         <td><%= "#{ticket.user.first_name}" "#{ticket.user.last_name}" %></td>
         <td><%= link_to "View", ticket_path(ticket) %></td>
       </tr>
        <% end %>
     </tbody>
   </table>

</div>
</div>

</div>
